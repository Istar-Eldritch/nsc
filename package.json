{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server/server.js",
    "start:dev": "nodemon",
    "build": "NODE_ENV=production webpack --mode production --progress",
    "build:dev": "webpack --mode development --config webpack.config.js --progress",
    "clean": "rm -fr dist && rm -fr .cache",
    "lint": "eslint --cache './src/**/*.{ts,tsx}'",
    "prettier": "prettier './src/**/*.{ts,tsx}' './src/**/*.{css}'",
    "fmt": "npm run lint -- --fix && npm run prettier -- --write",
    "fmt:check": "npm run lint && npm run prettier -- --check"
  },
  "nodemonConfig": {
    "watch": [
      "src/server"
    ],
    "ext": "ts,tsx,json",
    "ignore": [
      "dist",
      "node_modules"
    ],
    "exec": "ts-node --files --ignore node_modules src/server/index.dev.ts"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.6.0",
    "@fortawesome/fontawesome-free": "5.10.2",
    "@types/koa": "2.0.49",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-router": "7.0.42",
    "@types/koa-static-cache": "5.1.0",
    "@types/koa-webpack": "5.0.1",
    "@types/prismjs": "1.16.0",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "4.3.5",
    "@types/webpack": "4.39.1",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "babel-loader": "8.0.6",
    "babel-polyfill": "6.26.0",
    "css-loader": "3.2.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-standard": "4.0.1",
    "koa-webpack": "5.2.4",
    "less": "3.10.3",
    "less-loader": "5.0.0",
    "mini-css-extract-plugin": "0.8.0",
    "nodemon": "1.19.2",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "parcel": "1.12.3",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "pirates": "4.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "react-hot-loader": "4.12.14",
    "sass": "1.22.10",
    "style-loader": "1.0.0",
    "tailwindcss": "1.1.2",
    "terser-webpack-plugin": "2.1.0",
    "ts-node": "8.4.1",
    "typescript": "3.5.3",
    "webpack": "4.41.0",
    "webpack-bundle-analyzer": "3.5.1",
    "webpack-cli": "3.3.9",
    "webpack-dev-middleware": "3.7.1",
    "webpack-manifest-plugin": "2.1.1"
  },
  "dependencies": {
    "autoprefixer": "9.6.1",
    "axios": "0.19.0",
    "funfix": "7.0.1",
    "jsx-to-string": "1.4.0",
    "koa": "2.8.1",
    "koa-bodyparser": "4.2.1",
    "koa-router": "7.4.0",
    "koa-static-cache": "5.1.2",
    "postcss-modules": "1.4.1",
    "prismjs": "1.17.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "winston": "3.2.1"
  }
}
