{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url /static",
    "clean": "rm -fr dist && rm -fr .cache",
    "lint": "eslint --cache './src/**/*.{ts,tsx}'",
    "prettier": "prettier './src/**/*.{ts,tsx}' './src/**/*.{css,less,scss}'",
    "fmt": "npm run lint -- --fix && npm run prettier -- --write",
    "fmt:check": "npm run lint && npm run prettier -- --check"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "5.10.2",
    "@types/prismjs": "1.16.0",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "4.3.5",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "babel-polyfill": "6.26.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-react": "7.14.3",
    "parcel": "1.12.3",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "prettier": "1.18.2",
    "sass": "1.22.10",
    "tailwindcss": "1.1.2",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "autoprefixer": "9.6.1",
    "axios": "0.19.0",
    "funfix": "7.0.1",
    "jsx-to-string": "1.4.0",
    "postcss-modules": "1.4.1",
    "prismjs": "1.17.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1"
  }
}
